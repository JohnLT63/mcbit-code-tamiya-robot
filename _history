                    <shadow type=\"math_number\">\n                                            <field name=\"NUM\">0</field>\n                                          </shadow>\n                                          <block type=\"math_arithmetic\">\n                                            <field name=\"OP\">MULTIPLY</field>\n                                            <value name=\"A\">\n                                              <shadow type=\"math_number\">\n                                                <field name=\"NUM\">0</field>\n                                              </shadow>\n                                              <block type=\"variables_get\">\n                                                <field name=\"VAR\" id=\"Q+Lug?/utN.!R|`#znR!\" variabletype=\"\">DataH</field>\n                                              </block>\n                                            </value>\n                                            <value name=\"B\">\n                                              <shadow type=\"math_number\">\n                                                <field name=\"NUM\">256</field>\n                                              </shadow>\n                                            </value>\n                                          </block>\n                                        </value>\n                                        <value name=\"B\">\n                                          <shadow type=\"math_number\">\n                                            <field name=\"NUM\">0</field>\n                                          </shadow>\n                                          <block type=\"variables_get\">\n                                            <field name=\"VAR\" id=\"N=%Ac,f,F-cHZ7F1,[HJ\" variabletype=\"\">DataL</field>\n                                          </block>\n                                        </value>\n                                      </block>\n                         